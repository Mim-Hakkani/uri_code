#include<iostream>
#include<stdio.h>
using namespace std;
int main()
{
  int p_code,quantity;
  float total;
  cin>>p_code>>quantity;
  switch(p_code)
  {
     case 1:
      total=4.00*quantity;
      printf("Total: R$ %0.2f\n",total);
      break;

      case 2:
      total=4.50*quantity;
      printf("Total: R$ %0.2f\n",total);
      break;

      case 3:
      total=5.00*quantity;
      printf("Total: R$ %0.2f\n",total);
      break;
      case 4:
      total=2.00*quantity;
      printf("Total: R$ %0.2f\n",total);
      break;
      case 5:
      total=1.50*quantity;
      printf("Total: R$ %0.2f\n",total);
      break;

      default:
        printf("Invalid Input\n");


  }

}

