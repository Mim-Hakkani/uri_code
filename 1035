#include<iostream>
#include<stdio.h>
using namespace std;
int main()
{
    int a,b,c,d,s1,s2,p;
    cin>>a>>b>>c>>d;
    s1=a+b;
    s2=c+d;
    if(c>0&&d>0&&a%2==0&&b>c&&d>a&&s2>s1)
    {
        cout<<"Valores aceitos"<<endl;

    }
    else
    {
        cout<<"Valores nao aceitos"<<endl;
    }


}
